{
  "description": "Testing extension for Firefox",
  "manifest_version": 2,
  "name": "testext",
  "version": "1.3",
  "homepage_url": "https://github.com/9r3i/testext",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "testext.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "testext@test.mozilla.org",
      "strict_min_version": "58.0",
      "data_collection_permissions": {
        "required": [
          "locationInfo"
        ],
        "optional": [
          "technicalAndInteraction"
        ]
      }
    }
  }
}